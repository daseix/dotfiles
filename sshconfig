# ssh configuration for NEW rmc git server
Host rmc-github rmc-github.robotic.dlr.de
#ProxyCommand ssh -q seid_da@donau.robotic.dlr.de nc %h %p
HostName rmc-github.robotic.dlr.de
IdentityFile ~/.ssh/id_rsa_rmc
PreferredAuthentications publickey
PasswordAuthentication no
ForwardX11 no
ForwardAgent yes

# ssh configuration for OLD rmc git server
Host rmc-git rmc-git.robotic.dlr.de
ProxyCommand ssh -q seid_da@donau.robotic.dlr.de nc %h %p
HostName rmc-git.robotic.dlr.de
IdentityFile ~/.ssh/id_rsa_rmc
PreferredAuthentications publickey
PasswordAuthentication no
ForwardX11 no
ForwardAgent yes

