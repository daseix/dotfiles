# ssh configuration for rmc git server
Host rmc-git rmc-github.robotic.dlr.de
    #ProxyCommand ssh -q seid_da@donau.robotic.dlr.de nc %h %p
    HostName rmc-github.robotic.dlr.de
    IdentityFile ~/.ssh/id_rsa_rmc
    PreferredAuthentications publickey
    PasswordAuthentication no
    ForwardX11 no
    ForwardAgent yes


# ssh configuration for rmc svn access from outside
Host rmc-svn rmsvn01.robotic.dlr.de
    ProxyCommand ssh -q seid_da@donau.robotic.dlr.de nc %h %p
    #HostName rmc-git.robotic.dlr.de
    IdentityFile ~/.ssh/id_rsa
    PreferredAuthentications publickey
    PasswordAuthentication yes
    ForwardX11 no
    ForwardAgent yes


# ssh configuration for github
Host github github.com
    HostName github.com
    IdentityFile ~/.ssh/id_rsa_github
    PreferredAuthentications publickey
    PasswordAuthentication yes
    ForwardX11 no
    ForwardAgent yes

#
# Note:
#    pattern matching in 'host' field: first match overrides others
#
# host names containing dots:
#
Host *.robotic.dlr.de
    #ForwardAgent no
    ForwardX11 yes
    ForwardX11Trusted yes
    Compression no
    FallBackToRsh no
    Protocol 2
    ControlPath ~/.ssh/S.%r@%h:%p
    ControlMaster auto
    ControlPersist 120m




#
# others are considered to be internet hosts:
#
Host *.*
    #ForwardAgent no
    ForwardX11 yes
    FallBackToRsh no
    Protocol 2
    Compression no

#
# special treatment of localhost: 
#    -- 'NoHostAuthenticationForLocalhost' is a OpenSSH 3.x feature!
#
Host localhost
    NoHostAuthenticationForLocalhost yes
    #ForwardAgent no
    ForwardX11 yes
    ForwardX11Trusted yes
    Compression no
    FallBackToRsh no
    Protocol 2

#
# everything else which does not need a dot to be resolved:
#
Host *
    #ForwardAgent no
    ForwardX11 yes
    ForwardX11Trusted yes
    FallBackToRsh no
    Protocol 2
    Compression no
